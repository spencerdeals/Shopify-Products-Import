# Apify API Key (Primary scraping service)
APIFY_API_KEY=your_apify_api_key

# OpenAI for GPT parsing (Fallback)
OPENAI_API_KEY=your_openai_api_key

# UPCitemdb for product dimensions (Optional)
UPCITEMDB_API_KEY=your_upcitemdb_api_key

# Shopify Integration
SHOPIFY_DOMAIN=your-store.myshopify.com
SHOPIFY_ACCESS_TOKEN=your_shopify_access_token

# Server Configuration
PORT=3000
ADMIN_PASSWORD=1064
SHIPPING_RATE_PER_CUBIC_FOOT=12
MAX_CONCURRENT_SCRAPES=3

# Database (for order tracking)
TURSO_DATABASE_URL=your_turso_database_url
TURSO_AUTH_TOKEN=your_turso_auth_token