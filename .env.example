# Environment Variables Example
# Copy this file to .env and fill in your actual API keys

# Apify API Key - Get from https://console.apify.com/account/integrations
APIFY_API_KEY=YOUR_APIFY_API_KEY

# Zyte API Key - Get from https://app.zyte.com/o/
ZYTE_API_KEY=YOUR_ZYTE_API_KEY

# UPCitemdb API Key - Get from https://www.upcitemdb.com/
UPCITEMDB_API_KEY=YOUR_UPCITEMDB_API_KEY

# OpenAI API Key - Get from https://platform.openai.com/api-keys
OPENAI_API_KEY=YOUR_OPENAI_API_KEY

# ScrapingBee API Key - Get from https://app.scrapingbee.com/
SCRAPINGBEE_API_KEY=YOUR_SCRAPINGBEE_API_KEY

# OxyLabs API Credentials - Get from https://oxylabs.io/
OXYLABS_USERNAME=YOUR_OXYLABS_USERNAME
OXYLABS_PASSWORD=YOUR_OXYLABS_PASSWORD

# Admin Panel Credentials
ADMIN_USERNAME=admin
ADMIN_PASSWORD=1064

# Server Configuration
PORT=8080
NODE_ENV=development